{
	"name": "DailyScrum",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "yarn test:lint && yarn test:flow && yarn test:unit",
		"test:lint": "eslint . --quiet",
		"test:unit": "jest",
		"test:flow": "flow",
		"format": "find ./src -name '*.js' | xargs ./node_modules/.bin/prettier --write",
		"deploy": "./bin/code-push.sh --android --ios",
		"deploy:android": "./bin/code-push.sh --android",
		"deploy:ios": "./bin/code-push.sh --ios",
		"deploy:info:staging": "code-push deployment history DailyScrum-Android Staging && code-push deployment history DailyScrum-iOS Staging",
		"deploy:info:prod": "code-push deployment history DailyScrum-Android Production && code-push deployment history DailyScrum-iOS Production",
		"deploy:staging": "yarn deploy:staging:android && yarn deploy:staging:ios",
		"deploy:staging:android": "bundle exec fastlane android deploy --env=staging",
		"deploy:staging:ios": "bundle exec fastlane ios deploy --env=staging",
		"deploy:prod": "yarn deploy:prod:android && yarn deploy:prod:ios",
		"deploy:prod:android": "bundle exec fastlane android deploy --env=prod",
		"deploy:prod:ios": "bundle exec fastlane ios deploy --env=prod",
		"init:web": "yarn remove react && yarn add react react-dom react-native-web@0.1 react-native-web-linear-gradient@1.0 bodymovin@4.11",
		"stop:web": "git stash && yarn",
		"start:web": "./node_modules/.bin/webpack-dev-server --port 3000 --config web/webpack.config.babel.js --inline --colors",
		"build:web": "NODE_ENV=production ./node_modules/.bin/webpack -p --config web/webpack.config.babel.js"
	},
	"dependencies": {
		"@yfuks/react-native-action-sheet": "^0.0.4",
		"chroma-js": "^1.3.4",
		"date-fns": "^1.28.4",
		"easy-lottie-react-native": "^2.0.1",
		"lodash": "^4.17.4",
		"lottie-react-native": "^2.2.0",
		"react": "16.0.0-beta.5",
		"react-native": "0.49.3",
		"react-native-animatable": "^1.2.3",
		"react-native-code-push": "^5.1.3-beta",
		"react-native-firebase": "^3.0.4",
		"react-native-linear-gradient": "^2.3.0",
		"react-native-pathjs-charts": "git+ssh://git@github.com/capitalone/react-native-pathjs-charts.git#3267c55cb1134ca57ad4cb7c1df720a0c307c270",
		"react-native-safari-view": "^2.0.0",
		"react-native-splash-screen": "^3.0.0",
		"react-native-tab-view": "^0.0.69",
		"react-native-vector-icons": "^4.3.0",
		"react-navigation": "git+ssh://git@github.com/Minishlink/react-navigation.git#330f4e85799f6bcc9f78e5e04d3e86ada2160321",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"redux-persist": "^4.9.1",
		"redux-saga": "^0.15.6"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.0.1",
		"babel-loader": "^7.1.2",
		"babel-plugin-lodash": "^3.2.11",
		"babel-plugin-module-resolver": "^2.7.1",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.10",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-react-native": "^4.0.0",
		"babel-preset-react-native-stage-0": "^1.0.1",
		"eslint": "^4.5.0",
		"eslint-config-prettier": "^2.3.0",
		"eslint-config-react-app": "^2.0.1",
		"eslint-plugin-flowtype": "^2.35.0",
		"eslint-plugin-import": "^2.7.0",
		"eslint-plugin-jsx-a11y": "^6.0.2",
		"eslint-plugin-prettier": "^2.2.0",
		"eslint-plugin-react": "^7.3.0",
		"file-loader": "^1.1.5",
		"flow-bin": "^0.53.0",
		"html-webpack-plugin": "^2.30.1",
		"jest": "^21.2.1",
		"mockdate": "^2.0.2",
		"prettier": "^1.5.3",
		"redux-saga-test-plan": "^3.2.0",
		"webpack": "^3.8.1",
		"webpack-dev-server": "^2.9.3"
	},
	"jest": {
		"preset": "react-native"
	},
	"greenkeeper": {
		"ignore": [
			"react",
			"react-native",
			"flow-bin"
		]
	},
	"rnpm": {
		"assets": [
			"assets/native"
		]
	}
}
